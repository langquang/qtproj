<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Adding Animation" package="org.openfl.samples.addinganimation" version="1.0.0" company="OpenFL Technologies" />
	<app main="Main" path="Export" file="AddingAnimation" />
	<window width="640" height="480" background="#FFFFFF" fps="100" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	
	<assets path="Assets/images" rename="images" include="*.png|*.jpg" exclude="*.svg" />
	<assets path="assets/texts" rename="texts" include="*.xml" type="text" />
	<assets path="assets/fonts" rename="fonts" include="*.ttf" />
	<assets path="assets/sounds" rename="sounds" include="*.mp3" if="flash" />
	<assets path="assets/sounds" rename="sounds" include="*.wav|*.ogg" unless="flash" />
	<assets path="assets/modules" rename="modules" include="*.bin"/>
	<icon path="Assets/openfl.svg" />
	
</project>